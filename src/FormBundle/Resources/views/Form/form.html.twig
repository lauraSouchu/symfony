{{ form_start(form) }}
<div class="form-group">
    {{ form_row(form.email) }}
</div>
<div class="form-group">
    {{ form_row(form.nom) }}
</div>
<div class="form-group">
    {{ form_row(form.prenom) }}
</div>
<div class="form-group">
    {{ form_row(form.genre) }}
</div>
{%for adresse in form.listadresses%}
    <div class="form-group">
        {{ form_row(adresse.age) }}
    </div>
    <div class="form-group">
        {{ form_row(adresse.commentaire) }}
    </div>
    <div class="form-group">
        {{ form_row(adresse.adresse) }}
    </div>
    <div class="form-group">
        {{ form_row(adresse.cp) }}
    </div>
    <div class="form-group">
        {{ form_row(adresse.ville) }}
    </div>
{%endfor%}

{{ form_end(form)}}